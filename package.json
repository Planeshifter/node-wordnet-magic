{
  "name": "wordnet-magic",
  "version": "0.3.0",
  "description": "tools for working with Princeton's lexical database WordNet",
  "main": "src/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Planeshifter/node-wordnet-magic"
  },
  "keywords": [
    "WordNet",
    "semantic-web",
    "NLP",
    "lexical-database"
  ],
  "author": "Philipp Burckhardt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Planeshifter/node-wordnet-magic/issues"
  },
  "homepage": "https://github.com/Planeshifter/node-wordnet-magic",
  "dependencies": {
    "bluebird": "^2.2.2",
    "mysql": "^2.0.0",
    "sqlite3": "^3.0.5",
    "underscore": "^1.6.0",
    "validate.io-array": "^1.0.6"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "chai-as-promised": "5.x.x",
    "chai-things": "^0.2.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.8",
    "jshint": "^2.6.3",
    "jshint-stylish": "2.x.x",
    "mocha": "2.x.x"
  }
}
